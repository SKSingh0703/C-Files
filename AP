#include <stdio.h>
int main(){
    int i,a,d,n;
    printf("Enter the first term of the AP:");
    scanf("%d",&a);
    printf("Enter the common difference of the AP:");
    scanf("%d",&d);
    printf("Enter the number of terms required  of the AP:");
    scanf("%d",&n);
    for(i=1;i<=n;i=i+1){
        int x=a+(i-1)*d;
        printf("%d\n",x);
    }
}