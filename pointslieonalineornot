#include <stdio.h>
int main(){
    int x1,y1,x2,y2,x3,y3;
    printf("Input the coordinates in order of x1,y1,x2,y2,x3,y3:");
    scanf("%d %d %d %d %d %d",&x1,&y1,&x2,&y2,&x3,&y3);
    int m1,m2;
    m1=(y2-y1)/(x2-x1);
    m2=(y3-y1)/(x3-x2);
    if(m1==m2){
        printf("The input points lie on a line");
    }
    else{
        printf("Dont lie on a line");
    }
    
}